<plugin>
   <extension
         point="org.eclipse.emf.diffmerge.ui.modelComparisonContext">
      <scopeDefinition
            factory="org.eclipse.emf.diffmerge.ui.sirius.SiriusScopeFactory">
      </scopeDefinition>
      <comparisonMethod
            factory="org.eclipse.emf.diffmerge.ui.sirius.SiriusComparisonMethodFactory">
      </comparisonMethod>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.emf.ecore.xmi"
            file-extensions="aird,airdfragment"
            id="org.eclipse.emf.diffmerge.sirius.aird.content-type"
            name="Sirius diagram files"
            priority="normal">
      </content-type>
   </extension>

</plugin>
